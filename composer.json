{
  "name": "laravel-rocket/service-auth",
  "description": "Service Authentication for Laravel Rocket",
  "type": "library",
  "repositories": [
    {
      "type": "path",
      "url": "../foundation",
      "options": {
        "symlink": false
      }
    },
    {
      "type": "path",
      "url": "../generator",
      "options": {
        "symlink": false
      }
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Takaaki Mizuno",
      "email": "takaaki.mizuno@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/tinker": "^2.7",
    "laravel/passport": "^12.3",
    "nickdnk/graph-sdk": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "^11.9",
    "laravel-rocket/foundation": "@dev",
    "phpunit/phpunit": "^10.1",
    "friendsofphp/php-cs-fixer": "^3.25",
    "fakerphp/faker": "^1.23",
    "laravel/laravel": "^11.1"
  },
  "autoload": {
    "psr-4": {
      "LaravelRocket\\ServiceAuthentication\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
